<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DevModeFragment">

 <LinearLayout
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:orientation="vertical">
     <TextView
         android:id="@+id/tvTemperatureValueDM"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="8dp"
         android:text="NULL"
         android:textColor="@color/green_30"
         android:textSize="24sp" />
     <TextView
         android:id="@+id/tvSpeedValueDm"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="8dp"
         android:text="NULL"
         android:textColor="@color/green_30"
         android:textSize="24sp" />
     <TextView
         android:id="@+id/tvTemperatureValueBatteryDm"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="8dp"
         android:text="NULL"
         android:textColor="@color/green_30"
         android:textSize="24sp" />
     <TextView
         android:id="@+id/tvVoltageValueDM"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="8dp"
         android:text="NULL"
         android:textColor="@color/green_30"
         android:textSize="24sp" />
     <TextView
         android:id="@+id/tvPlugInValueDM"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginTop="8dp"
         android:text="NULL"
         android:textColor="@color/green_30"
         android:textSize="24sp" />



 </LinearLayout>
</FrameLayout>